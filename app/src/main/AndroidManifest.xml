<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Interview.geek">
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Interviewer.SendEmailActivity"></activity>
        <activity android:name=".Interviewer.AvailableInterviewActivity" />
        <activity android:name=".Interviewer.MySlotActivity" />
        <activity
            android:name=".Interviewer.InterviewDash"
            android:label="Interview Dash" />
        <activity
            android:name=".Interviewer.InterviewApplication"
            android:label="Schedule Interview" />
        <activity android:name=".Blog.PostDetails" />
        <activity android:name=".User.ProfileEditActivity" />
        <activity android:name=".Auth.SignupActivity" />
        <activity
            android:name=".User.ProfileActivity"
            android:label="Profile" />
        <activity
            android:name=".Contest.Reminder.UpcomingContestActivity"
            android:label="Upcoming contests" />
        <activity
            android:name=".Blog.BlogFeedActivity"
            android:label="Blogs" />
        <activity
            android:name=".Blog.BlogWriting"
            android:label="Write a blog" />
        <activity
            android:name=".MainActivity"
            android:label="Dashboard" />
        <activity android:name=".Auth.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>